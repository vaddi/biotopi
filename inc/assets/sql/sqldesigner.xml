-- 
-- BiotoPi SQLite Database Schema
-- 

CREATE TABLE `news` (
  `id` integer NOT NULL PRIMARY KEY,
  `title` text NOT NULL,
  `content` text NOT NULL,
  `author` integer NULL,
  `created` text NULL DEFAULT NULL,
  `updated` text NOT NULL DEFAULT "0000-00-00 00:00:00",
  `start` text NULL DEFAULT NULL,
  `end` text NULL DEFAULT NULL
);



